<% layout("/layouts/boilerplate") %>
<body>
    <h3> Please Fill Place Details</h3>
    <form action="/listings" method="POST">
        Title: <input type="text" name="listings[title]">
        description: <textarea name="listings[description]">Place Detalis here</textarea>
        image: <input type="text" name="listings[image]" placeholder="Paste Image Url Here!">
        Set Price: <input type="number" name="listings[price]">
        Location: <input type="text" name="listings[location]">
        Country: <input type="text" name="listings[country]">
        <button>Confirm Details</button>
    </form>
    
</body>
